@model  List<Park_Database.Model.Park_Time>

@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    var Search = ViewBag.Search;
    var libs = Model;
    //this.Model
}
<br /><br />
<form class="form-inline" method="post" action="~/Manager/Search">
    <div class="form-group">

        <input type="text" class="form-control" placeholder="名稱"
               name="search" id="search"
               value="@ViewBag.Search">
    </div>

    <button type="submit" class="btn btn-default">搜尋</button>
</form>
<hr />
<div class="list-group">
    <a href="#" class="list-group-item active">
        預約資訊
    </a>
    @for (int i = 0; i < Model.Count; i++)
    {

        <a class="list-group-item">
            <h4 class="list-group-item-heading">車牌:@Model[i].Time_Carname</h4>
            <p class="list-group-item-text">座號:@Model[i].Time_Seat</p>
            <p class="list-group-item-text">預約時間:@Model[i].Time_Start</p>
            <p class="list-group-item-text">截止時間:@Model[i].Time_End</p>
        </a>

    }



</div>